{
  "name": "pacher-studio",
  "version": "0.1.0",
  "description": "pacherでAHCのテストケース実行を支援するためのデスクトップアプリです",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc && vite build",
    "build:main": "tsc --project tsconfig.main.json",
    "build:renderer": "vite build",
    "start": "yarn build && electron .",
    "dev": "yarn build && electron . --dev",
    "dev:renderer": "vite",
    "watch": "tsc --watch",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" --max-warnings=0",
    "lint:fix": "eslint \"./src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"./src/**/*.{ts,tsx,js,jsx,json,css,html}\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "allotment": "^1.20.4",
    "glob": "^11.0.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "recharts": "^2.15.3",
    "smol-toml": "^1.3.4",
    "uuid": "^11.1.0",
    "zod": "^3.22.0",
    "cheerio": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "electron": "^25.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "eslint": "^8.52.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.0"
  },
  "author": "yunix-kyopro"
}
